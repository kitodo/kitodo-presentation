<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:flexform.sheet_general</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.pregeneration>
                        <TCEforms>
                            <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.basket.flexform.preGeneration</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.pregeneration>
                    <settings.pdfgenerate>
                        <TCEforms>
                            <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.basket.flexform.pdfGenerate</label>
                            <config>
                                <type>input</type>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </settings.pdfgenerate>
                    <settings.pdfdownload>
                        <TCEforms>
                            <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.basket.flexform.pdfDownload</label>
                            <config>
                                <type>input</type>
                                <eval>required</eval>
                                <default></default>
                            </config>
                        </TCEforms>
                    </settings.pdfdownload>
                    <settings.pdfprint>
                        <TCEforms>
                            <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.basket.flexform.pdfPrint</label>
                            <config>
                                <type>input</type>
                                <eval>required</eval>
                                <default></default>
                            </config>
                        </TCEforms>
                    </settings.pdfprint>
                    <settings.pdfparams>
                        <TCEforms>
                            <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.basket.flexform.pdfParams</label>
                            <config>
                                <type>input</type>
                                <eval>required</eval>
                                <default>##docId##,##startpage##,##endpage##,##startx##,##starty##,##endx##,##endy##,##rotation##</default>
                            </config>
                        </TCEforms>
                    </settings.pdfparams>
                    <settings.pdfparamseparator>
                        <TCEforms>
                            <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.basket.flexform.pdfParamSeparator</label>
                            <config>
                                <type>input</type>
                                <eval>required</eval>
                                <default>*</default>
                            </config>
                        </TCEforms>
                    </settings.pdfparamseparator>
                    <settings.basketGoToButton>
                        <TCEforms>
                            <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.basket.flexform.basketGoToButton</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.basketGoToButton>
                    <settings.targetBasket>
                        <TCEforms>
                            <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:flexform.targetBasket</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.targetBasket>
                </el>

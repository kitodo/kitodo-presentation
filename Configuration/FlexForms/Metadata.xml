<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:flexform.sheet_general</sheetTitle>
                <type>array</type>
                <el>
                    <settings.excludeOther>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:flexform.excludeOther</label>
                        <config>
                            <type>check</type>
                            <default>1</default>
                        </config>
                    </settings.excludeOther>
                    <settings.linkTitle>
                        <onChange>reload</onChange>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.linkTitle</label>
                        <config>
                            <type>check</type>
                            <default>1</default>
                        </config>
                    </settings.linkTitle>
                    <settings.targetPid>
                        <displayCond>FIELD:settings.linkTitle:REQ:true</displayCond>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:flexform.targetPidPageView</label>
                        <config>
                            <type>group</type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>0</minitems>
                            <show_thumbs>1</show_thumbs>
                            <wizards>
                                <suggest>
                                    <type>suggest</type>
                                </suggest>
                            </wizards>
                        </config>
                    </settings.targetPid>
                    <settings.getTitle>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:flexform.getTitle</label>
                        <config>
                            <type>check</type>
                            <default>1</default>
                        </config>
                    </settings.getTitle>
                    <settings.showFull>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.showFull</label>
                        <config>
                            <type>check</type>
                            <default>1</default>
                        </config>
                    </settings.showFull>
                    <settings.rootline>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.rootline</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <numIndex index="0">LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:text.none</numIndex>
                                    <numIndex index="1">0</numIndex>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <numIndex index="0">LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:text.all</numIndex>
                                    <numIndex index="1">1</numIndex>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <numIndex index="0">LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.rootline.titledata</numIndex>
                                    <numIndex index="1">2</numIndex>
                                </numIndex>
                            </items>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>1</minitems>
                            <default>0</default>
                        </config>
                    </settings.rootline>
                    <settings.originalIiifMetadata>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.originalIiifMetadata</label>
                        <onChange>reload</onChange>
                        <config>
                            <type>check</type>
                            <default>0</default>
                        </config>
                    </settings.originalIiifMetadata>
                    <settings.iiifMetadataWrap>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.iiifMetadataWrap</label>
                        <displayCond>FIELD:settings.originalIiifMetadata:=:1</displayCond>
                        <config>
                            <type>text</type>
                            <default>key.wrap = &lt;dt&gt;|&lt;/dt&gt;&#13;value.required = 1&#13;value.wrap = &lt;dd&gt;|&lt;/dd&gt;</default>
                        </config>
                    </settings.iiifMetadataWrap>
                    <settings.displayIiifDescription>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.displayIiifDescription</label>
                        <displayCond>FIELD:settings.originalIiifMetadata:=:1</displayCond>
                        <config>
                            <type>check</type>
                            <default>1</default>
                        </config>
                    </settings.displayIiifDescription>
                    <settings.displayIiifRights>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.displayIiifRights</label>
                        <displayCond>FIELD:settings.originalIiifMetadata:=:1</displayCond>
                        <config>
                            <type>check</type>
                            <default>1</default>
                        </config>
                    </settings.displayIiifRights>
                    <settings.displayIiifLinks>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.displayIiifLinks</label>
                        <displayCond>FIELD:settings.originalIiifMetadata:=:1</displayCond>
                        <config>
                            <type>check</type>
                            <default>1</default>
                        </config>
                    </settings.displayIiifLinks>
                    <settings.separator>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.metadata.flexform.separator</label>
                        <config>
                            <type>input</type>
                            <eval>required,trim</eval>
                            <default>#</default>
                        </config>
                    </settings.separator>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>

                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>0</minitems>
                            <show_thumbs>1</show_thumbs>
                            <wizards>
                                <suggest>
                                    <type>suggest</type>
                                </suggest>
                            </wizards>
                        </config>
                    </settings.targetBasket>
                    <settings.multiViewType>
                        <exclude>1</exclude>
                        <label>LLL:EXT:dlf/Resources/Private/Language/locallang_be.xlf:plugins.pageview.flexform.multiViewType</label>
                        <config>
                            <type>input</type>
                            <eval>nospace</eval>
                            <default></default>
                        </config>
                    </settings.multiViewType>
